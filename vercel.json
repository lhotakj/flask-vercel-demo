{
  "version": 2,
  "regions": [
    "fra1"
  ],
  "env": {
    "environment": "VERCEL",
    "redis": "localhost"
  },
  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "headers": [
    {
      "source": "*",
      "headers": [
        {
          "key": "X-Application",
          "value": "vercel"
        }
      ]
    }
  ]
}
